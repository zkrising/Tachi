[private]
interactive:
	-@cd ../ && just

# Run the client interactively. Changes made to files will trigger reloads.
start:
	pnpm dev

# Test that the client passes typechecking and linting.
test:
	pnpm typecheck
	pnpm lint

# Clean up the code and fix any automatically fixable mistakes.
lintfix:
	pnpm lint-fix
