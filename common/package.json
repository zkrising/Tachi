{
	"name": "tachi-common",
	"version": "2.2.0",
	"description": "Common Types & Utilities for Tachi",
	"main": "./js/index.js",
	"exports": {
		".": "./js/index.js",
		"./*": "./js/*.js"
	},
	"scripts": {
		"test": "tap",
		"build": "tsc -b",
		"install": "tsc -b",
		"prepublish": "tsc -b",
		"lint": "eslint . --ext .ts --fix"
	},
	"author": "zkldi",
	"license": "MIT",
	"dependencies": {
		"@types/express-serve-static-core": "4.17.29",
		"@types/mongodb": "3.6.20",
		"deepmerge": "^4.2.2",
		"prudence": "0.10.0",
		"zod": "^3.20.2"
	},
	"devDependencies": {
		"@types/tap": "^15.0.7",
		"tap": "15.1.6"
	},
	"nyc": {
		"reporter": [
			"html",
			"lcov"
		],
		"all": true,
		"include": [
			"src/*/!(test-utils)/**/*.ts"
		],
		"exclude": [
			"src/**/*.test.ts",
			"src/external/mongo/schemas.ts"
		]
	}
}