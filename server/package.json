		"winston": "3.5.1",
		"winston-daily-rotate-file": "4.6.0",
		"winston-transport": "4.5.0"
	},
	"nyc": {
		"reporter": [
			"html",
			"lcov"
		],
		"all": true,
		"include": [
			"src/*/!(test-utils)/**/*.ts"
		],
		"exclude": [
			"src/**/*.test.ts",
			"src/external/mongo/schemas.ts"
		]
	}
}